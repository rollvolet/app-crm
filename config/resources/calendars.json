{
  "version": "0.1",
  "prefixes": {
    "dct": "http://purl.org/dc/terms/",
    "ncal": "http://www.semanticdesktop.org/ontologies/2007/04/02/ncal#",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#",
    "crm": "http://data.rollvolet.be/vocabularies/crm/",
    "schema": "http://schema.org/",
    "time": "http://www.w3.org/2006/time#"
  },
  "resources": {
    "calendar-days": {
      "name": "calendar-day",
      "class": "time:DateTimeDescription",
      "attributes": {
        "day": {
          "type": "integer",
          "predicate": "time:day"
        },
        "month": {
          "type": "integer",
          "predicate": "time:month"
        },
        "year": {
          "type": "integer",
          "predicate": "time:year"
        },
        "unit-type": {
          "type": "url",
          "predicate": "time:unitType"
        },
        "comment": {
          "type": "string",
          "predicate": "dct:description"
        },
        "visit-status": {
          "type": "url",
          "predicate": "crm:visitStatus"
        },
      },
      "relationships": {
        "calendar": {
          "predicate": "nie:hasPart",
          "target": "calendar",
          "cardinality": "one",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.rollvolet.be/calendar-days/",
      "features": ["include-uri"]
    },
    "time-slots": {
      "name": "time-slot",
      "class": "ncal:NcalDateTime",
      "attributes": {
        "start": {
          "type": "datetime",
          "predicate": "ncal:dtstart"
        },
        "end": {
          "type": "datetime",
          "predicate": "ncal:dtend"
        },
        "title": {
          "type": "string",
          "predicate": "ncal:summary"
        },
        "description": {
          "type": "string",
          "predicate": "ncal:description"
        }
      }
      "relationships": {
        "calendar": {
          "predicate": "ncal:component",
          "target": "calendar",
          "cardinality": "one",
          "inverse": true
        },
        "request": {
          "predicate": "ncal:dateTime",
          "target": "request",
          "cardinality": "one",
          "inverse": true
        },
        "intervention": {
          "predicate": "ncal:dateTime",
          "target": "intervention",
          "cardinality": "one",
          "inverse": true
        },
        "order": {
          "predicate": "ncal:dateTime",
          "target": "order",
          "cardinality": "one",
          "inverse": true
        },
        "employee": {
          "predicate": "ncal:attendee",
          "target": "employee",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.rollvolet.be/time-slots/",
      "features": ["include-uri"]
    },
    "calendar-events": {
      "name": "calendar-event",
      "class": "ncal:Event",
      "attributes": {
        "ms-identifier": {
          "type": "string",
          "predicate": "ncal:uid",
          "_comment": "MS Object identifier of the calendar event"
        },
        "date": {
          "type": "date",
          "predicate": "ncal:date",
          "_comment": "Should actually be modelled with intermediate ncal:NcalDate resource"
        },
        "subject": {
          "type": "string",
          "predicate": "ncal:summary"
        },
        "description": {
          "type": "string",
          "predicate": "ncal:description"
        },
        "location": {
          "type": "string",
          "predicate": "ncal:location"
        },
        "url": {
          "type": "url",
          "predicate": "ncal:url"
        },
        "creator": {
          "type": "url",
          "predicate": "dct:creator"
        },
        "editor": {
          "type": "url",
          "predicate": "schema:editor"
        },
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        },
        "source": {
          "type": "string",
          "predicate": "dct:source",
          "_comment": "Either 'Access' or 'RKB'"
        }
      },
      "relationships": {
        "calendar": {
          "predicate": "ncal:component",
          "target": "calendar",
          "cardinality": "one",
          "inverse": true
        },
        "intervention": {
          "predicate": "dct:subject",
          "target": "intervention",
          "cardinality": "one"
        },
        "request": {
          "predicate": "dct:subject",
          "target": "request",
          "cardinality": "one"
        },
        "order": {
          "predicate": "dct:subject",
          "target": "order",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.rollvolet.be/calendar-events/",
      "features": ["include-uri"]
    },
    "calendars": {
      "name": "calendar",
      "class": "ncal:Calendar",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "nie:title"
        }
      },
      "relationships": {
        "calendar-days": {
          "predicate": "nie:hasPart",
          "target": "calendar-day",
          "cardinality": "many"
        },
        "calendar-events": {
          "predicate": "ncal:component",
          "target": "calendar-event",
          "cardinality": "many"
        },
        "time-slots": {
          "predicate": "ncal:component",
          "target": "time-slot",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.rollvolet.be/calendars/",
      "features": ["include-uri"]
    }
  }
}
