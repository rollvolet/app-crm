        "suffix": {
          "type": "string",
          "predicate": "vcard:hasHonorificSuffix"
        },
        "url": {
          "type": "url",
          "predicate": "vcard:hasUrl"
        },
        "comment": {
          "type": "string",
          "predicate": "vcard:hasNote"
        },
        "created": {
          "type": "dateTime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "dateTime",
          "predicate": "dct:modified"
        },
        "print-prefix": {
          "type": "boolean",
          "predicate": "crm:printPrefix"
        },
        "print-suffix": {
