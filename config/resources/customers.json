{
  "version": "0.1",
  "prefixes": {
    "dct": "http://purl.org/dc/terms/",
    "crm": "http://data.rollvolet.be/vocabularies/crm/",
    "gebouw": "https://data.vlaanderen.be/ns/gebouw#",
    "nco": "http://www.semanticdesktop.org/ontologies/2007/03/22/nco#",
    "schema": "http://schema.org/",
    "vcard": "http://www.w3.org/2006/vcard/ns#"
  },
  "resources": {
    "customers": {
      "name": "customer",
      "class": "vcard:VCard",
      "attributes": {
        "type": {
          "type": "url",
          "predicate": "dct:type",
          "_comment": "vcard:Individual or vcard:Organization"
        },
        "number": {
          "type": "integer",
          "predicate": "vcard:hasUID"
        },
        "prefix": {
          "type": "string",
          "predicate": "vcard:hasGivenName"
        },
        "name": {
          "type": "string",
          "predicate": "vcard:hasFamilyName"
        },
        "suffix": {
          "type": "string",
          "predicate": "vcard:hasHonorificSuffix"
        },
        "url": {
          "type": "url",
          "predicate": "vcard:hasUrl"
        },
        "vat-id": {
          "type": "string",
          "predicate": "schema:vatID"
        },
        "comment": {
          "type": "string",
          "predicate": "vcard:hasNote"
        },
        "memo": {
          "type": "string",
          "predicate": "crm:memo",
          "_comment": "Subproperty of vcard:hasNote"
        },
        "created": {
          "type": "dateTime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "dateTime",
          "predicate": "dct:modified"
        },
        "print-prefix": {
          "type": "boolean",
          "predicate": "crm:printPrefix"
        },
        "print-suffix": {
          "type": "boolean",
          "predicate": "crm:printSuffix"
        },
        "print-in-front": {
          "type": "boolean",
          "predicate": "crm:printSuffixInFront"
        }
      },
      "relationships": {
        "honorific-prefix": {
          "predicate": "vcard:hasHonorificPrefix",
          "target": "honorific-prefix",
          "cardinality": "one"
        },
        "address": {
          "predicate": "vcard:hasAddress",
          "target": "address",
          "cardinality": "one"
        },
        "language": {
          "predicate": "vcard:hasLanguage",
          "target": "language",
          "cardinality": "one"
        },
        "emails": {
          "predicate": "vcard:hasEmail",
          "target": "email",
          "cardinality": "many"
        },
        "telephones": {
          "predicate": "vcard:hasTelephone",
          "target": "telephone",
          "cardinality": "many"
        },
        "contacts": {
          "predicate": "nco:representative",
          "target": "contact",
          "cardinality": "many"
        },
        "buildings": {
          "predicate": "schema:affiliation",
          "target": "building",
          "cardinality": "many"
        },
        "cases": {
          "predicate": "schema:customer",
          "target": "case",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.rollvolet.be/customers/"
    },
    "contacts": {
      "name": "contact",
      "class": "nco:Contact",
      "attributes": {
        "number": {
          "type": "integer",
          "predicate": "vcard:hasUID"
        },
        "position": {
          "type": "integer",
