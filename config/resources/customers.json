        },
        "created": {
          "type": "dateTime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "dateTime",
          "predicate": "dct:modified"
        },
        "print-prefix": {
          "type": "boolean",
          "predicate": "crm:printPrefix"
        },
        "print-suffix": {
          "type": "boolean",
          "predicate": "crm:printSuffix"
        },
        "print-in-front": {
          "type": "boolean",
          "predicate": "crm:printSuffixInFront"
        }
      },
      "relationships": {
        "honorific-prefix": {
          "predicate": "vcard:hasHonorificPrefix",
          "target": "honorific-prefix",
          "cardinality": "one"
        },
        "address": {
          "predicate": "vcard:hasAddress",
          "target": "address",
          "cardinality": "one"
        },
        "language": {
          "predicate": "vcard:hasLanguage",
          "target": "language",
          "cardinality": "one"
        },
        "emails": {
          "predicate": "vcard:hasEmail",
          "target": "email",
          "cardinality": "many"
        },
        "telephones": {
          "predicate": "vcard:hasTelephone",
          "target": "telephone",
          "cardinality": "many"
        },
        "cases": {
          "predicate": "crm:contact",
          "target": "case",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.rollvolet.be/contacts/"
    },
    "buildings": {
      "name": "building",
      "class": "gebouw:Gebouw",
      "attributes": {
        "number": {
          "type": "integer",
          "predicate": "vcard:hasUID"
        },
