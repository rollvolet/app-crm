{
  "version": "0.1",
  "prefixes": {
    "dct": "http://purl.org/dc/terms/",
    "nfo": "http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#",
    "dbpedia": "http://dbpedia.org/resource/",
    "dossier": "https://data.vlaanderen.be/ns/dossier#",
    "prov": "http://www.w3.org/ns/prov#"
  },
  "resources": {
    "files": {
      "name": "file",
      "class": "nfo:FileDataObject",
      "attributes": {
        "filename": {
          "type": "string",
          "predicate": "nfo:fileName"
        },
        "type": {
          "type": "url",
          "predicate": "dct:type"
        },
        "format": {
          "type": "string",
          "predicate": "dct:format"
        },
        "size": {
          "type": "integer",
          "predicate": "nfo:fileSize"
        },
        "extension": {
          "type": "string",
          "predicate": "dbpedia:fileExtension"
        },
        "created": {
          "type": "datetime",
          "predicate": "nfo:fileCreated"
        }
      },
      "relationships": {
        "download": {
          "predicate": "nie:dataSource",
          "target": "remote-file",
          "cardinality": "one",
          "inverse": true
        },
        "case": {
          "predicate": "dossier:Dossier.bestaatUit",
          "target": "case",
          "cardinality": "one",
          "inverse": true
        },
        "accountancy-export": {
          "predicate": "prov:generated",
          "target": "accountancy-export",
          "cardinality": "one",
          "inverse": true
        },
        "invoice": {
          "predicate": "prov:wasDerivedFrom",
          "target": "invoice-document",
          "cardinality": "one"
        },
        "order": {
          "predicate": "prov:wasDerivedFrom",
          "target": "order",
          "cardinality": "one"
        },
        "offer": {
          "predicate": "prov:wasDerivedFrom",
          "target": "offer",
          "cardinality": "one"
        },
        "intervention": {
          "predicate": "prov:wasDerivedFrom",
          "target": "intervention",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.rollvolet.be/files/",
      "features": ["include-uri"]
    },
    "remote-files": {
      "name": "remote-file",
      "class": "nfo:RemoteDataObject",
      "_comment": "Subclass of nfo:FileDataObject",
      "attributes": {
        "filename": {
          "type": "string",
          "predicate": "nfo:fileName"
        },
        "format": {
          "type": "string",
          "predicate": "dct:format"
        },
        "size": {
          "type": "integer",
          "predicate": "nfo:fileSize"
        },
        "extension": {
          "type": "string",
          "predicate": "dbpedia:fileExtension"
        },
        "created": {
          "type": "datetime",
          "predicate": "nfo:fileCreated"
        },
        "ms-identifier": {
          "type": "string",
          "predicate": "dct:identifier"
        },
        "url": {
          "type": "url",
          "predicate": "nfo:fileUrl"
        }
      },
      "relationships": {
        "data-source": {
          "predicate": "nie:dataSource",
          "target": "file",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.rollvolet.be/files/",
      "features": ["include-uri"]
    }
  }
}
